nts/Generation.js","original":{"line":71,"column":27}},{"generated":{"line":121,"column":86},"source":"components/Generation.js","original":{"line":71,"column":0}},{"name":"componentConnector","generated":{"line":123,"column":15},"source":"components/Generation.js","original":{"line":73,"column":15}},{"generated":{"line":123,"column":33},"source":"components/Generation.js","original":{"line":73,"column":33}},{"name":"Generation","generated":{"line":123,"column":34},"source":"components/Generation.js","original":{"line":73,"column":34}},{"generated":{"line":123,"column":44},"source":"components/Generation.js","original":{"line":73,"column":33}}],"sources":{"components/Generation.js":"import React, { Component } from 'react';\nimport { connect } from 'react-redux';\nimport { generationActionCreator } from '../actions/generation';\n\nconst MINIMUM_DELAY = 3000;\n\nclass Generation extends Component {\n    timer = null; // Default value, define object that has not been set yet\n\n    // Fetch generation if component is loaded in DOM\n    componentDidMount() {\n        this.fetchNextGeneration();\n    }\n\n    // Stop loop if component is removed from DOM - destroy, cleanup here\n    componentWillUnmount() {\n        clearTimeout(this.timer); // removes reference of setTimeout\n    }\n\n    fetchGeneration = () => {\n        fetch('http://localhost:3000/generation')\n            .then(response => response.json())\n            .then(json => { \n                // console.log('json', json);\n                this.props.dispatchGeneration(json.generation);\n            })\n            .catch(error => console.error('error', error));\n    };\n\n    // Update frontend to display the updated generation\n    fetchNextGeneration = () => {\n        this.fetchGeneration();\n\n        let delay = new Date(this.props.generation.expiration).getTime() - new Date().getTime(); // get difference between this expiration time value and current time value\n\n        if (delay < MINIMUM_DELAY) {\n            delay = MINIMUM_DELAY;\n        };\n\n        this.timer = setTimeout(() => this.fetchNextGeneration(), delay); // Recursive function - calls itself and dynamically renews generation\n    }\n\n    render() {\n        console.log('this.props', this.props);\n\n        const { generation } = this.props;\n\n        return (\n            <div>\n                <h3>Generation {generation.generationId}. Expires on:</h3>\n                <h4>{new Date(generation.expiration).toString()}</h4>\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = state => {\n    const generation = state.generation;\n\n    return { generation };\n};\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        dispatchGeneration: generation => dispatch(\n            generationActionCreator(generation)\n        )\n    }\n};\n\nconst componentConnector = connect(mapStateToProps, mapDispatchToProps);\n\nexport default componentConnector(Generation);"},"lineCount":null}},"error":null,"hash":"939bf050bc1ad10a8afeae43d014e219","cacheData":{"env":{}}}